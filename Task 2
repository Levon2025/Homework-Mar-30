#include <stdio.h>

 int main(){
  const int size = 5;
  int arr[size];
  for(int i = 0; i < size;i++){
   scanf("%d",&arr[i]);
}

  int lowest = arr[0];
  int index = 0;
   for(int i = 0 ; i < size ; i++){
    if(lowest > arr[i]){
     lowest = arr[i];
     index = i;
}
}
  printf("%d %d",lowest,index);
  return 0;
}
